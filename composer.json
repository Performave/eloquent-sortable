{
    "name": "performave/eloquent-sortable",
    "description": "Sortable behaviour for eloquent models created by Spatie and forked by Performave",
    "homepage": "https://github.com/performave/eloquent-sortable",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be"
        },
        {
            "name": "Eric Wang",
            "email": "eric@performave.com"
        }
    ],
    "keywords":
        [
            "sort",
            "sortable",
            "eloquent",
            "model",
            "laravel",
            "behaviour"
        ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "nesbot/carbon": "^2.63",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.5",
        "orchestra/testbench": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Performave\\EloquentSortable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Performave\\EloquentSortable\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Performave\\EloquentSortable\\EloquentSortableServiceProvider"
            ]
        }
    }
}
